{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// import { getUserInfoAPI } from '@/api'\nimport { mapGetters } from 'vuex';\nexport default {\n  name: 'my-layout',\n  methods: {\n    // 退出按钮点击\n    quitFn() {\n      // 添加确认\n      this.$confirm('是否退出登录？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$message({\n          type: 'success',\n          message: '退出成功!'\n        }); // 清空token\n\n        this.$store.commit('updataToken', ''); // 清空用户信息\n\n        this.$store.commit('updateUserInfo', {}); // 跳转回登录页面\n\n        this.$router.push('/login');\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消退出'\n        });\n      });\n    },\n\n    handleOpen(key, keyPath) {\n      console.log(key, keyPath);\n    },\n\n    handleClose(key, keyPath) {\n      console.log(key, keyPath);\n    }\n\n  },\n  // 映射\n  computed: { ...mapGetters(['username', 'nickname', 'user_pic'])\n  } // 登录之后跳转到主页这里时，获取用户的信息\n  // (不要在这写，如果在这写，刷新主页，每次都会发送请求获取已经获取过的内容，浪费资源)\n  // 可以使用全局路由守卫\n  // created () {\n  //   this.$store.dispatch('initUserInfo')\n  //   console.log(1)\n  // }\n\n};","map":{"version":3,"mappings":";AA6CA;AACA;AACA;EACAA,iBADA;EAEAC;IACA;IACAC;MACA;MACA;QACAC,uBADA;QAEAC,sBAFA;QAGAC;MAHA,GAKAC,IALA,CAKA;QACA;UACAD,eADA;UAEAE;QAFA,GADA,CAKA;;QACA,sCANA,CAOA;;QACA,yCARA,CASA;;QACA;MACA,CAhBA,EAiBAC,KAjBA,CAiBA;QACA;UACAH,YADA;UAEAE;QAFA;MAIA,CAtBA;IAuBA,CA3BA;;IA4BAE;MACAC;IACA,CA9BA;;IA+BAC;MACAD;IACA;;EAjCA,CAFA;EAqCA;EACAE,YACA;EADA,CAtCA,CAyCA;EACA;EACA;EACA;EACA;EACA;EACA;;AA/CA","names":["name","methods","quitFn","confirmButtonText","cancelButtonText","type","then","message","catch","handleOpen","console","handleClose","computed"],"sourceRoot":"src/views/layout","sources":["index.vue"],"sourcesContent":["<template>\r\n  <el-container class=\"main-container\">\r\n    <el-header>\r\n      <img src=\"../../assets/images/标题logo(半留白).png\" alt=\"\" />\r\n\r\n      <!-- 搜索框 -->\r\n      <div class=\"search_container\">\r\n        <form action=\"\" class=\"\">\r\n            <input type=\"text\" class=\"search\" placeholder=\"搜索\">\r\n            <button class=\"btn\"><img src=\"../../assets/images/搜索图标.png\" alt=\"\"></button>\r\n            <div class=\"notice\"><img src=\"../../assets/images/通知图标.png\" alt=\"\"></div>\r\n        </form>\r\n      </div>\r\n\r\n    </el-header>\r\n    <el-container>\r\n      <el-aside width=\"200px\">\r\n        <div>\r\n              <div class=\"aside_img_name\">\r\n                <!-- 利用null判断运算符 省去v-if -->\r\n                <img\r\n                  :src=\"user_pic ?? require('../../assets/images/scene.jpg')\"\r\n                  alt=\"\"\r\n                />\r\n                <!-- 用 || 判断，不用??，因为空字符串不生效 -->\r\n                <div class=\"aside_textbox\" >\r\n                                  <span>欢迎{{ nickname || username }}</span>\r\n                                  <span>简介：</span>\r\n                </div>\r\n              </div>\r\n        </div>\r\n        <div class=\"exit\"><img src=\"../../assets/images/退出图标.png\" alt=\"\"><span>退出</span></div>\r\n        <!-- default-active:当前激活菜单的 index\r\n        -->\r\n\r\n      </el-aside>\r\n      <el-container>\r\n        <el-main>Main</el-main>\r\n        <el-footer>© www.张伟是混蛋.com </el-footer>\r\n      </el-container>\r\n    </el-container>\r\n  </el-container>\r\n</template>\r\n\r\n<script>\r\n// import { getUserInfoAPI } from '@/api'\r\nimport { mapGetters } from 'vuex'\r\nexport default {\r\n  name: 'my-layout',\r\n  methods: {\r\n    // 退出按钮点击\r\n    quitFn () {\r\n      // 添加确认\r\n      this.$confirm('是否退出登录？', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      })\r\n        .then(() => {\r\n          this.$message({\r\n            type: 'success',\r\n            message: '退出成功!'\r\n          })\r\n          // 清空token\r\n          this.$store.commit('updataToken', '')\r\n          // 清空用户信息\r\n          this.$store.commit('updateUserInfo', {})\r\n          // 跳转回登录页面\r\n          this.$router.push('/login')\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消退出'\r\n          })\r\n        })\r\n    },\r\n    handleOpen (key, keyPath) {\r\n      console.log(key, keyPath)\r\n    },\r\n    handleClose (key, keyPath) {\r\n      console.log(key, keyPath)\r\n    }\r\n  },\r\n  // 映射\r\n  computed: {\r\n    ...mapGetters(['username', 'nickname', 'user_pic'])\r\n  }\r\n  // 登录之后跳转到主页这里时，获取用户的信息\r\n  // (不要在这写，如果在这写，刷新主页，每次都会发送请求获取已经获取过的内容，浪费资源)\r\n  // 可以使用全局路由守卫\r\n  // created () {\r\n  //   this.$store.dispatch('initUserInfo')\r\n  //   console.log(1)\r\n  // }\r\n}\r\n</script>\r\n\r\n<style  lang=\"less\" scoped>\r\n.main-container {\r\n  height: 100%; //找父\r\n\r\n  .el-aside{\r\n    background-color: #ffffff;\r\n  }\r\n  .el-aside {\r\n    >div:first-child {\r\n      height: 27%;\r\n      background-color: #77bdee;\r\n      //解决父子margin-top塌陷问题\r\n      overflow: hidden;\r\n      // display: flex;\r\n      // justify-content: center;\r\n      // border-top: 1px solid #000;\r\n      // border-bottom: 1px solid #000;\r\n      .aside_img_name{\r\n        margin-top: 2.3rem;\r\n        // width: 100%;\r\n        // height: 45%;\r\n        display: flex;\r\n        // background-color: red;\r\n        img {\r\n                margin-left: 1.5rem;\r\n                align-items: center;\r\n                // align-self: center;\r\n                border-radius: 50%;\r\n                background-color: white;\r\n                width: 50px;\r\n                height: 50px;\r\n                // margin-right: 26px;\r\n              }\r\n        .aside_textbox{\r\n          margin-left: .6rem;\r\n          //  align-items: center;\r\n          display: inline-block;\r\n          span {\r\n            margin-top: .2rem;\r\n            display: block;\r\n            // align-self: center;\r\n            height: 20px;\r\n            line-height: 20px;\r\n            color: white;\r\n            font-size: 12px;\r\n          }\r\n        }\r\n\r\n      }\r\n\r\n      // .el-menu-vertical-demo{\r\n      //   border-right: none !important;\r\n      // }\r\n      // .el-submenu,\r\n      // .el-menu:hover{\r\n      //   background-color: aqua !important;\r\n      // }\r\n    }\r\n    .exit{\r\n      height: 30px;\r\n      border-radius: 1rem;\r\n      background-color: #cae9ff;\r\n      img{\r\n        width: 20%;\r\n        height: 100%;\r\n      }\r\n      span{\r\n        height: 100%;\r\n        line-height: 30px;\r\n      }\r\n    }\r\n  }\r\n  .el-header {\r\n    height: 65px !important;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    background-color: rgb(85, 156, 206);\r\n    img{\r\n      // margin-right: 2rem;\r\n      width: 20%;\r\n    }\r\n    .search_container{\r\n      // margin-left: 4rem;\r\n      // display: inline-block;\r\n      background-color: none;\r\n      height: 100%;\r\n      width: 200px;\r\n      display: flex;\r\n      justify-content: center;\r\n      align-items:center;\r\n      // flex-direction:column;\r\n      ::-webkit-input-placeholder {\r\n        color: #676767;\r\n        padding-left: .8rem;\r\n      }\r\n      .search{\r\n        align-self:center;\r\n        background: none;\r\n        outline: none;\r\n        border: none;\r\n        border: 2px solid #676767;\r\n        border-radius: 1rem;\r\n        height: 1.5rem;\r\n        //失去聚焦时候的颜色\r\n        color: pink;\r\n        font-size: 1rem;\r\n        position: relative;\r\n      }\r\n      .search:focus{\r\n        //聚焦时候的颜色\r\n        color: #676767;\r\n      }\r\n      .btn{\r\n\r\n            position: absolute;\r\n            top: 22px;\r\n            /* left: -54px; */\r\n            right: 2.1rem;\r\n            // background-color: pink;\r\n            // background: url(\"../../assets/images/搜索图标.png\") no-repeat;\r\n            width: 33px;\r\n            height: 21px;\r\n            border: none;\r\n            background: none;\r\n            img{\r\n                width: 100%;\r\n                height: 100%;\r\n            }\r\n      }\r\n      .btn:hover{\r\n        cursor: pointer;\r\n      }\r\n      .notice{\r\n        position: absolute;\r\n                    top: 14px;\r\n            /* left: -54px; */\r\n            right: 17rem;\r\n            width: 36px;\r\n            // background-color: red;\r\n            height: 36px;\r\n            display: flex;\r\n            align-items:center;\r\n            img{\r\n\r\n              // margin: 0 auto;\r\n              width: 100%;\r\n              height: 100%;\r\n            }\r\n            img:hover{\r\n              cursor: pointer;\r\n            }\r\n    }\r\n    }\r\n\r\n  }\r\n  .el-footer {\r\n    background-color: #eee;\r\n    font-size: 12px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n}\r\n.avatar {\r\n  border-radius: 50%;\r\n  width: 35px;\r\n  height: 35px;\r\n  background-color: #fff;\r\n  margin-right: 10px;\r\n  object-fit: cover;\r\n}\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}